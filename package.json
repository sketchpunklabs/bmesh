{
    "name": "bmesh",
    "version": "0.0.1",
    "author": "Pedro Sousa ( Vor @ SketchPunk Labs )",
    "description": "",
    "keywords": [
        "mesh editing",
        "threejs",
        "radial edge",
        "half edge",
        "modeling"
    ],
    "license": "MIT",
    "homepage": "https://github.com/sketchpunklabs/bmesh#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sketchpunklabs/bmesh.git"
    },
    "bugs": {
        "url": "https://github.com/sketchpunklabs/bmesh/issues"
    },
    "files": [
        "dist"
    ],
    "main": "./dist/bmesh.cjs.js",
    "module": "./dist/bmesh.es.js",
    "types": "./dist/bmesh.d.ts",
    "exports": {
        ".": {
            "import": "./dist/bmesh.es.js",
            "require": "./dist/bmesh.cjs.js"
        }
    },
    "scripts": {
        "dev": "vite --config vite.config.js",
        "build": "vite build",
        "build:types": "tsc --declaration --noEmit false --emitDeclarationOnly --strict false --outDir ./dist",
        "build:site": "vite build --mode site",
        "preview-site": "vite preview",
        "lint": "eslint . --ext .ts"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^6.8.0",
        "@typescript-eslint/parser": "^6.8.0",
        "eslint": "^8.51.0",
        "typescript": "^5.2.2",
        "three": "^0.148.0",
        "vite": "^4.5.0",
        "vite-plugin-list-directory-contents": "^1.4.5"
    }
